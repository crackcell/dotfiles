[user]
	name = Menglong TAN
	email = tanmenglong@gmail.com
[alias]
	ci = commit -a
	st = status
	br = branch
	co = checkout
	df = diff
	lg = log -p
[core]
	excludesfile = /Users/crackcell/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[color]
	ui = auto
[push]
	default = simple
